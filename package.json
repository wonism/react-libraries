{
  "name": "@w-labs/react-libs",
  "version": "0.0.0",
  "description": "Monorepo for React Libraries",
  "workspaces": [
    "packages/*"
  ],
  "main": "n/a",
  "repository": "https://github.com/wonism/react-libs.git",
  "author": "wonism <yocee57@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "lint": "eslint packages/**/src --ext ts,tsx",
    "lint:fix": "yarn lint --fix",
    "sb": "start-storybook -p 6006",
    "doc": "build-storybook",
    "precanary": "yarn build",
    "canary": "lerna publish --canary",
    "prebuild": "rm -rf ./packages/**/lib",
    "build:cjs": "./scripts/build.sh cjs",
    "build:esm": "./scripts/build.sh esm",
    "build": "yarn build:cjs && yarn build:esm",
    "dev:cjs": "yarn build:cjs --watch",
    "dev:esm": "yarn build:esm --watch",
    "dev": "yarn dev:esm && tsc -b -w",
    "prerelease": "yarn build",
    "release": "lerna publish"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-docs": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/react": "^6.1.11",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/jest": "^26.0.19",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "core-js": "^3.8.1",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
